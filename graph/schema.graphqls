# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  payrollSummary(year: Int!, month: Int!, country: Country!): [PayrollSummary!]
}

type PayrollSummary {
  gross: Float!
  net: Float!
  bonus: Float
  user: User!
}

type User {
  firstName: String!
  lastName: String!
}

enum Country {
  FRANCE, ITALY
}


